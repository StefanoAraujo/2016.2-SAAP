Host *
  User ubuntu
  # User root
  UserKnownHostsFile /dev/null
  StrictHostKeyChecking no
  PasswordAuthentication no
  IdentitiesOnly yes
  LogLevel FATAL


Host digitalocean
  Hostname 10.0.0.2 
  Port 22
  IdentityFile .vagrant/machines/digitalocean/virtualbox/private_key
  # IdentityFile ~/.ssh/id_rsa
